<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.java.admin.mapper.SysDictMapper">
    <resultMap type="com.java.admin.pojo.SysDictPojo" id="SysDictPojoMap">
        <result property="code" column="code" jdbcType="VARCHAR"/>
        <result property="description" column="description" jdbcType="VARCHAR"/>
        <result property="value" column="value" jdbcType="VARCHAR"/>
    </resultMap>

    <select id="selectDictAll" resultMap="SysDictPojoMap">
--         select t1.code,
--                t1.value,
--                t2.description
--         from dictionary_value t1
--         left join dictionary_code t2 on t2.id = t1.code_value_id
SELECT code,value,description FROM dictionary_value;
    </select>
</mapper>